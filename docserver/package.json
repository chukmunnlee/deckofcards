{
  "name": "docserver",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch -e 'node --require=./dist/otel'",
    "start:devinst": "INSTRUMENTATION=yes nest start --watch -e 'node --require=./dist/otel'",
    "start:debug": "INSTRUMENTATION=yes nest start --debug --watch -e 'node --require=./dist/otel'",
    "start:prod": "INSTRUMENTATION=yes node --require=./dist/otel dist/main",
    "start:prod-load": "nest build && INSTRUMENTATION=yes node --require=./dist/otel dist/main --decksDir ../assets/ --drop",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@nestjs/common": "^11.1.2",
    "@nestjs/core": "^11.1.2",
    "@nestjs/platform-express": "^11.1.2",
    "@nestjs/serve-static": "^5.0.3",
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/auto-instrumentations-node": "^0.59.0",
    "@opentelemetry/exporter-prometheus": "^0.201.1",
    "@opentelemetry/exporter-trace-otlp-http": "^0.201.1",
    "@opentelemetry/instrumentation-express": "^0.50.0",
    "@opentelemetry/instrumentation-http": "^0.201.1",
    "@opentelemetry/instrumentation-mongodb": "^0.54.0",
    "@opentelemetry/instrumentation-nestjs-core": "^0.47.0",
    "@opentelemetry/instrumentation-winston": "^0.46.0",
    "@opentelemetry/resources": "^2.0.1",
    "@opentelemetry/sdk-metrics": "^2.0.1",
    "@opentelemetry/sdk-node": "^0.201.1",
    "@opentelemetry/sdk-trace-node": "^2.0.1",
    "@opentelemetry/semantic-conventions": "^1.34.0",
    "hbs": "^4.2.0",
    "js-yaml": "^4.1.0",
    "mongodb": "^6.16.0",
    "nest-winston": "^1.10.2",
    "reflect-metadata": "^0.2.2",
    "request-ip": "^3.3.0",
    "rxjs": "^7.8.2",
    "strftime": "^0.10.3",
    "swagger-ui-dist": "^5.22.0",
    "ulid": "^3.0.0",
    "uuid": "^11.1.0",
    "winston": "^3.17.0",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.27.0",
    "@nestjs/cli": "^11.0.7",
    "@nestjs/schematics": "^11.0.5",
    "@nestjs/testing": "^11.1.2",
    "@openapitools/openapi-generator-cli": "^2.20.2",
    "@swc/cli": "^0.7.7",
    "@swc/core": "^1.11.29",
    "@types/express": "^5.0.2",
    "@types/jest": "^29.5.14",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^22.15.23",
    "@types/request-ip": "^0.0.41",
    "@types/strftime": "^0.9.8",
    "@types/supertest": "^6.0.3",
    "@types/uuid": "^10.0.0",
    "@types/yargs": "^17.0.33",
    "eslint": "^9.27.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.4.0",
    "globals": "^16.2.0",
    "jest": "^29.7.0",
    "source-map-support": "^0.5.21",
    "supertest": "^7.1.1",
    "ts-jest": "^29.3.4",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.33.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
